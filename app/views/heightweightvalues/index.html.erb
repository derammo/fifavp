<h1>Listing heightweightvalues</h1>

<table>
  <tr>
    <th>Heightweight</th>
    <th>Skill</th>
    <th>Value</th>
    <th class='internal'></th>
    <th class='internal'></th>
    <th class='internal'></th>
  </tr>

<% @heightweightvalues.each do |heightweightvalue| %>
  <tr>
    <td><%=h heightweightvalue.heightweight_id %></td>
    <td><%=h heightweightvalue.skill_id %></td>
    <td><%=h heightweightvalue.value %></td>
    <td class='internal'><%=link_to 'Show', heightweightvalue %></td>
    <td class='internal'><%=link_to 'Edit', edit_heightweightvalue_path(heightweightvalue) %></td>
    <td class='internal'><%=link_to 'Destroy', heightweightvalue, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

