<h1>Listing positioncoefficients</h1>

<table>
  <tr>
    <th>Position</th>
    <th>Skill</th>
    <th>Coefficient</th>
    <th class='internal'></th>
    <th class='internal'></th>
    <th class='internal'></th>
  </tr>

<% @positioncoefficients.each do |positioncoefficient| %>
  <tr>
    <td><%=h positioncoefficient.position_id %></td>
    <td><%=h positioncoefficient.skill_id %></td>
    <td><%=h positioncoefficient.coefficient %></td>
    <td class='internal'><%=link_to 'Show', positioncoefficient %></td>
    <td class='internal'><%=link_to 'Edit', edit_positioncoefficient_path(positioncoefficient) %></td>
    <td class='internal'><%=link_to 'Destroy', positioncoefficient, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Positioncoefficient', new_positioncoefficient_path %>
