<h1>Listing rolevalues</h1>

<table>
  <tr>
    <th>Role</th>
    <th>Skill</th>
    <th>Value</th>
    <th class='internal'></th>
    <th class='internal'></th>
    <th class='internal'></th>
  </tr>

<% @rolevalues.each do |rolevalue| %>
  <tr>
    <td><%=h rolevalue.role_id %></td>
    <td><%=h rolevalue.skill_id %></td>
    <td><%=h rolevalue.value %></td>
    <td class='internal'><%=link_to 'Show', rolevalue %></td>
    <td class='internal'><%=link_to 'Edit', edit_rolevalue_path(rolevalue) %></td>
    <td class='internal'><%=link_to 'Destroy', rolevalue, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Rolevalue', new_rolevalue_path %>
